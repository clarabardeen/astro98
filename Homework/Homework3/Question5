# rotating digits
def rotate_digits(n1, n2): 
    n1 = str(n1) 
    n2 = str(n2) 
    n1 += n2 
    return int(n1) 
a = 5 
b = 1234
print(rotate_digits(a,b)) 
# worked but I had to convert to a string, so tried another way

def rotate_digits (N=12345, K=1): 
    N = 12345 
    n1 = N % 10 # 5 
    n2 = N // 10 #1234 
    return n1, n2 
def rotatedigits (n1, n2): 
    return int("{}{}".format(n1, n2))
a = 5 
b = 1234 
print(rotatedigits(a, b))
