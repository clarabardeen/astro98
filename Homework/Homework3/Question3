# check leap year, conditional
def leapyear(year): 
    if year % 4 == 0: 
        if year % 100 ==0: 
            if year % 400 ==0: 
                print (f"{year} is TRUE") 
            else: 
                print(f"{year} is FALSE") 
        else: 
            print(f"{year} is TRUE")
    else: 
        print(f"{year} is FALSE") 
print(leapyear(2024))
